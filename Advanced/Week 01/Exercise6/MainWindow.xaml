<Window x:Class="Exercise6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Exercise6"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ComboBox x:Name="comboBox" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,10,0,0" Width="150">
            <ComboBoxItem>Console Application</ComboBoxItem>
            <ComboBoxItem>Windows Form</ComboBoxItem>
            <ComboBoxItem IsSelected="True">WPF Application</ComboBoxItem>
        </ComboBox>
        <ListBox x:Name="listBox" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="175,10,0,0" Width="150">
            <ListBoxItem>Console Application</ListBoxItem>
            <ListBoxItem>Windows Form</ListBoxItem>
            <ListBoxItem IsSelected="True">WPF Application</ListBoxItem>
        </ListBox>
        
    </Grid>
</Window>
