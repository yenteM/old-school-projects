        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Oef_10._8"
        mc:Ignorable="d"
        Title="Wekkerwindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="alarmTimeTextBox" HorizontalAlignment="Left" Height="23" Margin="249,98,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <Button x:Name="setButton" Content="Set alarm" HorizontalAlignment="Left" Margin="285,198,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Label x:Name="timeLabel" FontSize="30" HorizontalAlignment="Left" Margin="127,28,0,0" VerticalAlignment="Top" Width="272" Height="62"/>
        <TextBox x:Name="beepTimeTextBox" HorizontalAlignment="Left" Height="23" Margin="249,129,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <Label x:Name="alarmTimeLabel" Content="Alarm tijd:" HorizontalAlignment="Left" Margin="127,95,0,0" VerticalAlignment="Top" Width="117"/>
        <Label x:Name="beepTimeLabel" Content="Beep Time:" HorizontalAlignment="Left" Margin="127,126,0,0" VerticalAlignment="Top" Width="117"/>

    </Grid>
</Window>
